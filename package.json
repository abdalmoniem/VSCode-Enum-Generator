{
	"name": "enum-generator",
	"publisher": "AbdAlMoniem-AlHifnawy",
	"displayName": "Enum Generator",
	"description": "Generate / Convert enums with values with hamming distances between them",
	"version": "1.9.3",
	"engines": {
		"vscode": "^1.66.0"
	},
	"author": {
		"email": "hifnawy_moniem@hotmail.com",
		"name": "AbdAlMoniem AlHifnawy",
		"url": "https://abdalmoniem.github.io"
	},
	"os": [
		"Windows",
		"Linux",
		"MacOS"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/abdalmoniem/C-Call-Hierarchy"
	},
	"bugs": {
		"email": "hifnawy_moniem@hotmail.com",
		"url": "https://github.com/abdalmoniem/C-Call-Hierarchy/issues"
	},
	"categories": [
		"Snippets",
		"Programming Languages"
	],
	"keywords": [
		"C",
		"C++",
		"Snippets"
	],
	"icon": "media/extension_icon.png",
	"activationEvents": [
		"onLanguage:c",
		"onLanguage:cpp"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands":[
			{
				"category": "Enum Generator",
				"command": "enum-generator.generateEnum",
				"title": "Generate An Enum"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.66.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.16.0",
		"@typescript-eslint/parser": "^5.16.0",
		"eslint": "^8.11.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.2",
		"typescript": "^4.5.5",
		"@vscode/test-electron": "^2.1.3"
	}
}